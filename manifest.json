{
  "manifest_version": 2,
  "name": "Space Cats",
  "description": "Space cats taking over your computer",
  "version": "1.0",
  "icons": {
    "cat_pic": "cat_in_space.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery-1.8.1.min.js",
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
      "cat_in_space.png"
  ]
}
